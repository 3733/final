<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXListView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="778.0" prefWidth="319.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <AnchorPane prefHeight="107.0" prefWidth="200.0">
         <children>
            <HBox prefHeight="108.0" prefWidth="241.0">
               <children>
                  <VBox prefHeight="108.0" prefWidth="152.0">
                     <children>
                        <JFXButton contentDisplay="CENTER" onAction="#emergencyButton" ripplerFill="#8e8e8e" text="Emergency">
                           <font>
                              <Font name="Montserrat Regular" size="12.0" />
                           </font>
                           <VBox.margin>
                              <Insets left="25.0" />
                           </VBox.margin>
                        </JFXButton>
                     </children>
                  </VBox>
               </children>
            </HBox>
         </children>
      </AnchorPane>
      <HBox alignment="TOP_CENTER" prefHeight="51.0" prefWidth="242.0">
         <children>
            <JFXButton buttonType="RAISED" onAction="#clear" prefHeight="23.0" prefWidth="50.0" ripplerFill="#8e8e8e" style="-fx-background-color: #1a479a;" text="Clear " textFill="WHITE">
               <font>
                  <Font name="Montserrat Regular" size="12.0" />
               </font>
            </JFXButton>
         </children>
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="29.0" prefWidth="242.0">
