<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="275.0" prefWidth="272.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.AdminDrawerController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <HBox alignment="TOP_CENTER" prefHeight="36.0" prefWidth="228.0">
               <children>
                  <Label alignment="TOP_CENTER" text="Settings">
                     <font>
                        <Font name="Montserrat Regular" size="16.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton contentDisplay="CENTER" onAction="#serviceRequest" prefHeight="28.0" prefWidth="206.0" ripplerFill="#8e8e8e" text="Create A Service Request">
                     <font>
                        <Font name="Montserrat Regular" size="14.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton contentDisplay="CENTER" onAction="#acceptRequest" ripplerFill="#8e8e8e" text="Existing Service Requests">
                     <font>
                        <Font name="Montserrat Regular" size="14.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton contentDisplay="CENTER" onAction="#edit" prefHeight="23.0" prefWidth="156.0" ripplerFill="#8e8e8e" text="Edit Map">
                     <font>
                        <Font name="Montserrat Regular" size="14.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
            <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
               <children>
                  <JFXButton contentDisplay="CENTER" onAction="#editUsers" prefHeight="23.0" prefWidth="152.0" ripplerFill="#8e8e8e" text="Edit Users">
                     <font>
                        <Font name="Montserrat Regular" size="14.0" />
                     </font>
                  </JFXButton>
               </children>
            </HBox>
         </children>
         <VBox.margin>
            <Insets top="25.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
