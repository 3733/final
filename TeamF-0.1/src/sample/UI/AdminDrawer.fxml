            <Label fx:id="sendLabel" prefHeight="26.0" prefWidth="151.0" text="Send Directions by Email" visible="false">
               <font>
                  <Font name="Montserrat Regular" size="12.0" />
               </font>
               <HBox.margin>
                  <Insets bottom="15.0" />
               </HBox.margin>
            </Label>
         </children>
         <VBox.margin>
            <Insets top="15.0" />
         </VBox.margin>
      </HBox>
      <VBox fx:id="adminBox" prefHeight="200.0" prefWidth="100.0" visible="false">
